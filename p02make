OBJ = p02.o p02lex.o


p02:	${OBJ}
	g++  -o p02  ${OBJ} -ly -lm

p02.o:	p02.cpp p02lex.h
	g++ -c -g p02.cpp
p02lex.cpp:	p02lex.l
	lex p02lex.l
	mv lex.yy.c p02lex.cpp
p02lex.o:	p02lex.cpp p02lex.h
	g++ -c -g p02lex.cpp

clean:	
	rm *.o
	rm p02lex.cpp
	rm p02
	